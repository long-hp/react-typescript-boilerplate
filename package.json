{
  "name": "react-typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "eslint": "eslint src/**/*.{ts,tsx} --quiet --fix",
    "test:nowatch": "cross-env CI=true yarn test --env=jsdom",
    "prettier": "prettier --ignore-path .eslintignore --parser json --write",
    "test-all": "yarn eslint && yarn test:nowatch",
    "tsc": "tsc",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "release": "yarn test-all && zip -r ${npm_package_name}-build.zip build && release-it \"$1\" && rm ${npm_package_name}-build.zip",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "storybook-docs": "start-storybook --docs",
    "stories-eject": "stories-eject",
    "ce": "components-eject src",
    "rcg": "rc-generate"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn eslint",
      "git add"
    ],
    "*.{test.ts,test.tsx}": [
      "yarn test:nowatch",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "qs": "^6.9.1",
    "ramda": "^0.26.1",
    "react": "^17.0.1",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-redux": "^7.2.0",
    "react-redux-i18n": "^1.9.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.1",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "web-vitals": "^0.2.4",
    "wiloke-react-core": "0.7.9"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-jest": "5.3.19",
    "@storybook/addon-knobs": "^6.0.3",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storysource": "5.3.19",
    "@storybook/addon-viewport": "^6.0.4",
    "@storybook/addons": "^5.3.19",
    "@storybook/components": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@storybook/source-loader": "5.3.19",
    "@storybook/theming": "^6.0.21",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/qs": "^6.9.1",
    "@types/ramda": "^0.26.40",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.5",
    "@types/react-redux-i18n": "^0.0.11",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-persist": "^4.3.1",
    "@types/redux-saga": "^0.10.5",
    "@types/reselect": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@typescript-eslint/typescript-estree": "^4.14.0",
    "chromatic": "^5.1.0",
    "cross-env": "^6.0.3",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^3.1.0",
    "lint-staged": "8.1.7",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "rc-generate": "^0.5.0",
    "react-app-rewired": "^2.1.5",
    "react-test-renderer": "^16.12.0",
    "redux-logger": "^3.0.6",
    "typescript": "^4.0.3",
    "typescript-plugin-css-modules": "^2.1.2",
    "yarn": "^1.19.1"
  }
}
