{
  "name": "react-typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "eslint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "test:nowatch": "cross-env CI=true react-scripts test --env=jsdom",
    "prettier": "prettier --ignore-path .eslintignore --parser json --write",
    "test-all": "yarn eslint && yarn test:nowatch",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "release": "func() { yarn test-all && zip -r ${npm_package_name}-build.zip build && release-it \"$1\" && rm ${npm_package_name}-build.zip; }; func"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn eslint",
      "git add"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write",
      "git add"
    ],
    "*.{test.ts,test.tsx}": [
      "yarn test:nowatch",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-redux-i18n": "^1.9.3",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.9",
    "@types/react": "^16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-redux-i18n": "^0.0.11",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-persist": "^4.3.1",
    "@types/redux-saga": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "@typescript-eslint/typescript-estree": "^2.8.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "lint-staged": "8.1.7",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.5",
    "react-test-renderer": "^16.12.0",
    "redux-logger": "^3.0.6",
    "tslint": "^5.20.1",
    "tslint-import-rules": "^0.3.0",
    "tslint-lines-between-class-members": "^1.3.6",
    "typescript": "3.6.3",
    "yarn": "^1.19.1"
  }
}
